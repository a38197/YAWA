    android:layout_height="match_parent">

    <TextView
        android:id="@+id/yawa_provider_debug_item_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/yawa_provider_debug_item_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/yawa_provider_debug_item_stamp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/yawa_provider_debug_item_data"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

</LinearLayout>